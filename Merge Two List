package Linklist;

import Linklist.Sort_List.Node;

public class MergeList {
	Node head;

	class Node {
		int data;
		Node next;

		Node(int data) {
			this.data = data;
			next = null;
		}
	}

	void add(int data) {
		Node add = new Node(data);
		add.next = head;
		head = add;
	}

	void merge(Node head1, Node head2) {
		Node n = head1;
		Node curr = head2;

		while (n.next != null) {

			n = n.next;
			if (n.next == null) {
				while (curr.next != null) {
					n.next = curr;
					curr = curr.next;
					n = n.next;

				}
			}
		}
		Node current = head;
		Node nextnode = null;
		int temp;
		while (current != null) {
			nextnode = current;
			while (nextnode != null) {
				if (current.data > nextnode.data) {
					temp = current.data;
					current.data = nextnode.data;
					nextnode.data = temp;
				}
				nextnode = nextnode.next;
			}
			current = current.next;
		}

	}

	void traverse() {
		Node n = head;
		while (n != null) {
			System.out.print(n.data + " ");
			n = n.next;
		}
		System.out.println();
	}

	public static void main(String[] args) {
		MergeList list = new MergeList();
		MergeList list2 = new MergeList();
		list.add(1);
		list.add(2);
		list.add(6);
		list2.add(7);
		list2.add(9);
		list2.add(5);
		System.out.println("first list");
		list.traverse();
		System.out.println("second list");
		list2.traverse();
		list.merge(list.head, list2.head);
		System.out.println("final merged sorted list");
		list.traverse();

	}

}

Output: first list
        6 2 1 
        second list
        5 9 7 
        final merged sorted list
        1 2 5 6 7 9 
